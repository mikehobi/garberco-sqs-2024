<section class="project js-project">
    <h1 class="project__title h4 js-project-title">{title}</h1>
    <div class="project__images js-project-images">
        {.repeated section items}
            {.if customContent.diptychSmallLeft}
                <div class="project__image js-project-image">
                    {@|apply image.block}
                </div>
                {.if customContent.diptychImage}
                    <div class="project__image js-project-image">
                        foo
                        {customContent.diptychImage|apply image.block}
                    </div>
                {.end}
            {.or customContent.diptychSmallRight}
                
            {.or customContent.diptychLeanLeft}
                
            {.or customContent.diptychLeanRight}
                
            {.or customContent.diptychGutterCenter}
                
            {.or customContent.diptychFullCenter}
                
            {.or customContent.SingleSmallRight}
                
            {.or customContent.SingleSmallLeft}
                
            {.or customContent.SingleSmallCenter}
                
            {.or customContent.SingleHalfRightMargin}
                
            {.or customContent.SingleHalfLeftMargin}
                
            {.or customContent.SingleHalfRightEdge}
                
            {.or customContent.SingleHalfLeftEdge}
                
            {.or customContent.SingleHalfCenter}
                
            {.or customContent.SingleThreeQuarterCenter}
                
            {.or customContent.SingleFullWidth}
                
            {.end}
        {.end}
    </div>
</section>