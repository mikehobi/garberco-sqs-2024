<section class="homepage js-homepage grid js-grid">
    {.section collection}
        {.repeated section collections}
            <div class="grid__item expander-bottom">
                <a href="{fullUrl}" class="grid__photo js-project-tile animate js-animate js-router--ignore" data-title="{title}">
                    {.section mainImage}
                        {@|apply image.block}
                    {.end}
                </a>
            </div>
        {.end}
    {.end}
</section>