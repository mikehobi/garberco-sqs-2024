<div id="projectPages" data-collection-id="{id}">
    {.section collection}
        {.repeated section collections}
            <h1>{title}</h1>
            {.repeated section items}
                <img src="{assetUrl}" />
                {.section customContent.diptychImage}
                    <img src="{assetUrl}" />
                {.end}
            {.end}
        {.end}
    {.end}
</div>